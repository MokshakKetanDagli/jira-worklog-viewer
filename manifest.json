{
  "manifest_version": 3,
  "name": "Jira Worklog Viewer",
  "version": "1.0",
  "description": "View your Jira worklogs by date",
  "permissions": ["storage", "cookies"],
  "host_permissions": ["https://*.atlassian.net/*"],
  "action": {
    "default_popup": "popup/popup_calendar.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
